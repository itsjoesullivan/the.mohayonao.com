extend ../lib/layout

block config
  - $.title = "Formant Khoomii"
  - $.path  = "/khoomii"
  - $.jslist  = ["/lib/timbre.js", "index.js"]
  - $.csslist = ["index.css"]

block content
  h1 #{$.title}
  #container
    div(style="margin-bottom:25px")
      button(id="play").btn Play
    script(src="https://gist.github.com/mohayonao/5204491.js")
    p use:
      ul
        li: a(href="http://mohayonao.github.com/timbre.js") timbre.js
